{
    "compilerOptions": {
        "target": "es5",
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "paths": {
            "@svgs/*": ["./web/svgs/src/*"],
            "@util/*": ["./web/util/*"],
            "@react/*": ["./web/react/*"],
            "@static/*": ["./static/*"],
            "@ml/*": ["./web/ml/*"]
        }
    },
    "include": ["build/**/*","web/**/*", "static"]
}