{
    "compilerOptions": {
        "target": "ES6",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "paths": {
            "@svgs/*": ["./web/svgs/src/*"],
            "@util/*": ["./web/util/*"],
            "@react/*": ["./web/react/*"],
            "@static/*": ["./static/*"],
        }
    },
    "include": ["build/**/*","web/**/*", "static"]
}